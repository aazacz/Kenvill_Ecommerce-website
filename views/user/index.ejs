<script>
    $(document).ready(function() {

$('.addtocart').on('click', function () {
var idFrombutton = $(this).data('id');
console.log(idFrombutton);

const session = '<%=session %>';
console.log(session);
console.log("session Printing " + session);
console.log();

const data = {
_id: idFrombutton,
userid: session
};

if (session != 1) {
$.ajax({
url: `/addtocart`,
method: "put",
data: data,
success: function (response) {
    toastr.options = {
        "closeButton": false,
        "debug": false,
        "newestOnTop": false,
        "progressBar": false,
        "positionClass": "toast-top-right",
        "preventDuplicates": false,
        "showDuration": "200",
        "hideDuration": "500",
        "timeOut": "1000",
        "extendedTimeOut": "1000",
        "showEasing": "swing",
        "hideEasing": "linear",
        "showMethod": "fadeIn",
        "hideMethod": "fadeOut"
    };

    if (response.message == "product already added") {
        toastr["info"]("Product Already Added to your cart", "Don't worry");
    } else {
        toastr["success"]("Product Added to the cart", "Added");
    }
}
});

} else {
$.ajax({
url: `/login`,
method: "get",
data: data,
success: function (response) {
    console.log(response);
    if (response.title == "logged in") {
        $.ajax({
            url: `/addtocart`,
            method: "put",
            data: data,
            success: function (response) {
                toastr.options = {
                    "closeButton": false,
                    "debug": false,
                    "newestOnTop": false,
                    "progressBar": false,
                    "positionClass": "toast-top-right",
                    "preventDuplicates": false,
                    "showDuration": "200",
                    "hideDuration": "500",
                    "timeOut": "1000",
                    "extendedTimeOut": "1000",
                    "showEasing": "swing",
                    "hideEasing": "linear",
                    "showMethod": "fadeIn",
                    "hideMethod": "fadeOut"
                };

                if (response.message == "product already added") {
                    toastr["info"]("Product Already Added to your cart", "Don't worry");
                } else {
                    toastr["success"]("Product Added to the cart", "Added");
                }
            }
        });
    } else {
        // Redirect to login URL
        window.location.href = "/login?page=1";

    }
}
});
}
});
});

    
    </script>
